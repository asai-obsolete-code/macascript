;; core macro

;; you can perform some bad behavior on "env". it is intentional
(defmacro defmaca (name args &body body)
  (with-gensyms (s definition)
    `(defun ,name (,s env ,@args)
       (let ((,definition 
	      (progn
		,@body)))
	 (m-compile ,s env ,definition)))))


